{
  "name": "@s3pweb/node-otlp-tracer",
  "version": "1.0.1",
  "description": "A open telemetry tracer for node JS",
  "keywords": [
    "otlp",
    "node"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "lint": "eslint --fix ./src",
    "prepare": "husky install",
    "release": "commit-and-tag-version -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s3pweb/node-otlp-tracer.git"
  },
  "author": "s3pweb",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.4.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.41.0",
